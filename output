<script>
	$('#mobile-burger-patty').click( function(){
		
		if( $(this).hasClass('slip-in-active') ){
			
			$(this).removeClass('slip-in-active');
			$('#mobile-nav-slip').animate({left: '-40vw',opacity: 0},400).fadeOut(1);
			$('#wrapper').animate({left: '0vw'},400);
			$('#mobile-nav-ui').animate({left: '0vw'},400);
			$('#static-buttons').animate({bottom: '0vw'},400);
			$('#filtersPane-mobile').hide();
			$('#mobile-basenav').animate({bottom: '0px'});
			
		} else {
			
			$(this).addClass('slip-in-active');
			$('#mobile-nav-slip').fadeIn(1).animate({left: '0vw',opacity: 1},400);
			$('#wrapper').css({zIndex: 1}).animate({left: '80vw'},400);
			$('#mobile-nav-ui').animate({left: '80vw'},400);
			$('#static-buttons').animate({bottom: '-100px'},400);
			$('#mobile-basenav').animate({bottom: '-100px'});
			$('#filtersPane-mobile').hide();
		}
			
	});	
	
	$('#mobile-nav-search-toggle').click( function(){
		
		if( $(this).hasClass('slip-in-active') ){
			
			$(this).removeClass('slip-in-active');
			$('#filtersPane-mobile').animate({right: '-40vw',opacity: 0},200).fadeOut();
			$('#wrapper').animate({left: '0vw'},200);
			$('#mobile-nav-ui').animate({left: '0vw'},200);
			$('#static-buttons').animate({bottom: '0px'},200);
			$('#mobile-basenav').animate({bottom: '0px'});
			
		} else {
			
			$(this).addClass('slip-in-active');
			$('#filtersPane-mobile').fadeIn(1).animate({right: '0vw',opacity: 1},400);
			$('#wrapper').animate({left: '-80vw'},400);
			$('#mobile-nav-ui').animate({left: '-80vw'},400);
			$('#static-buttons').animate({bottom: '-100px'});
			$('#mobile-basenav').animate({bottom: '-100px'});
			
		}
			
	});	

</script>	
